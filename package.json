{
  "name": "@sanity/embeddings-index-ui",
  "version": "1.1.8",
  "description": "Various Sanity Studio plugins for integrating with the embeddings index API",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/embeddings-index-ui#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/embeddings-index-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/embeddings-index-ui.git"
  },
  "license": "MIT",
  "author": "Sanity <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "node": {
        "module": "./dist/index.esm.js",
        "import": "./dist/index.cjs.mjs"
      },
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sanity/icons": "^2.4.1",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.7.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@sanity/pkg-utils": "^2.4.8",
    "@sanity/plugin-kit": "^3.1.10",
    "@sanity/semantic-release-preset": "^4.1.4",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "sanity": "^3.16.1",
    "semantic-release": "^21.1.1",
    "styled-components": "^5.3.11",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3.14"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
